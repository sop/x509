{
    "name": "sop/x509",
    "description": "A PHP library for X.509 public key certificates, attribute certificates, certification requests and certification path validation.",
    "homepage": "https://github.com/sop/x509",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "x509",
        "x.509",
        "certificate",
        "attribute certificate",
        "ac",
        "certification request",
        "csr"
    ],
    "authors": [
        {
            "name": "Joni Kollani",
            "email": "joni.kollani@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2|^8.0",
        "ext-gmp": "*",
        "sop/asn1": "^4.0.0",
        "sop/x501": "^0.6.1",
        "sop/crypto-types": "^0.3.0",
        "sop/crypto-bridge": "^0.3.1",
        "sop/crypto-encoding": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "friendsofphp/php-cs-fixer": "^3.49"
    },
    "autoload": {
        "psr-4": {
            "Sop\\X509\\": "lib/X509/"
        }
    }
}
